{
  "expo": {
    "name": "Club de operativas de trading",
    "slug": "operationa-club",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://firebasestorage.googleapis.com/v0/b/webtradingsf.appspot.com/o/recurso%20web%2F512.png?alt=media&token=cfa7e433-c2f9-4888-a2f3-eaa97434c486",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "https://firebasestorage.googleapis.com/v0/b/webtradingsf.appspot.com/o/recurso%20web%2F512.png?alt=media&token=cfa7e433-c2f9-4888-a2f3-eaa97434c486",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cmarko.operationaclub",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "tg",
              "host": "resolve"
            },
            {
              "scheme": "metatrader5"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.QUERY_ALL_PACKAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cf500426-7b25-4dcd-aed6-f8bbf5736a69"
      }
    },
    "owner": "cmarko"
  }
}